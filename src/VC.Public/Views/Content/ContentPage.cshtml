@model VC.Public.Models.ContentPageViewModel

@section Meta {
    <title>@Html.Raw(Model.Title)</title>
    <meta name="keywords" content="@Html.Raw(Model.MetaKeywords)">
    <meta name="description" content="@Html.Raw(Model.MetaDescription)">
}
@if (!string.IsNullOrEmpty(Model.SocialMeta))
{
    @section SocialMeta {
        @Html.Raw(Model.SocialMeta)
    }
}

@section PageScripts {
    @Html.Raw(Model.Scripts)
}
@Html.Raw(Model.HTML)
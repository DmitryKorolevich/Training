{

  "webroot": "wwwroot",
  "version": "1.0.0-*",
  "buildOptions": {
    "compile": {
      "exclude": [
        "wwwroot",
        "node_modules",
        "bower_components",
        "deploy"
      ]
    },
    "copyToOutput": {
      "include": [
        "config.json",
        "config.local.json",
        "hosting.json",
        "web.config",
        "CSharpClassTemplate.tcs",
        "CSharpPreparseTemplate.tcs"
      ]
    },
    "preserveCompilationContext": true,
    "emitEntryPoint": true,
    "optimize": true
  },
  "runtimeOptions": {
    "gcServer": false,
    "gcConcurrent": true
  },
  "packOptions": {
    "exclude": [
      "node_modules",
      "bower_components",
      "deploy",
      "**.xproj",
      "**.user",
      "**.vspscc"
    ]
  },
  "publishOptions": {
    "include": [
      "wwwroot",
      "Views",
      "AppConfig",
      "config.json",
      "web.config",
      "CSharpClassTemplate.tcs",
      "CSharpPreparseTemplate.tcs"
    ]
  },
  "dependencies": {
    "cloudscribe.Web.Pagination": "1.0.2",
    "Microsoft.AspNetCore.Server.Kestrel": "1.0.0-*",
    "Microsoft.AspNetCore.Identity.EntityFrameworkCore": "1.0.0.*",
    "Microsoft.AspNetCore.Identity": "1.0.0.*",
    "Microsoft.AspNetCore.Server.IISIntegration": "1.0.0-*",
    "Microsoft.AspNetCore.Mvc": "1.0.0-*",
    "Microsoft.AspNetCore.Mvc.TagHelpers": "1.0.0-*",
    "Microsoft.AspNetCore.Diagnostics": "1.0.0-*",
    "Microsoft.AspNetCore.DataProtection": "1.0.0-*",
    "Microsoft.EntityFrameworkCore": "1.0.0.*",
    "Microsoft.AspNetCore.StaticFiles": "1.0.0-*",
    "Microsoft.AspNetCore.Http": "1.0.0-*",
    "Microsoft.AspNetCore.Session": "1.0.0-*",
    "Microsoft.Extensions.Configuration.Json": "1.0.0-*",
    "Microsoft.Extensions.Logging": "1.0.0-*",
    "Microsoft.Extensions.Logging.Console": "1.0.0-*",
    "Microsoft.Extensions.WebEncoders": "1.0.0-*",
    "Newtonsoft.Json": "9.0.1",
    "Microsoft.AspNetCore.Razor": "1.0.0-*",
    "VitalChoice.Ecommerce.Domain": "1.0.0-*",
    "VitalChoice.Infrastructure.Domain": "1.0.0-*",
    "VitalChoice.Data": "1.0.0-*",
    "VitalChoice.Infrastructure": "1.0.0-*",
    "VitalChoice.Core": "1.0.0-*",
    "VitalChoice.Validation": "1.0.0-*",
    "VitalChoice.SharedWeb": "1.0.0-*",
    "Microsoft.Extensions.Configuration.EnvironmentVariables": "1.0.0-*"
  },
  "frameworks": {
    "net451": {
    }
  },
  "tools": {
    "Microsoft.AspNetCore.Server.IISIntegration.Tools": {
      "version": "1.0.0-*",
      "imports": "portable-net45+wp80+win8+wpa81+dnxcore50"
    }
  },
  "scripts": {
    "postpublish": "dotnet publish-iis --publish-folder %publish:OutputPath% --framework %publish:FullTargetFramework%"
  }
}
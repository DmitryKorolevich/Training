{

  "webroot": "wwwroot",
  "version": "1.0.0-*",
  "buildOptions": {
    "compile": {
      "exclude": [
        "wwwroot",
        "node_modules",
        "bower_components",
        "deploy"
      ]
    }
  },
  "packOptions": {
    "exclude": [
      "node_modules",
      "bower_components",
      "deploy",
      "**.xproj",
      "**.user",
      "**.vspscc"
    ]
  },
  "dependencies": {
    "cloudscribe.Web.Pagination": "1.0.1",
    "Microsoft.AspNetCore.Server.Kestrel": "1.0.0-rc2-final",
    "Microsoft.AspNetCore.Server.IISIntegration": "1.0.0-rc2-final",
    "Microsoft.AspNetCore.Mvc": "1.0.0-rc2-final",
    "Microsoft.AspNetCore.Mvc.TagHelpers": "1.0.0-rc2-final",
    "Microsoft.AspNetCore.Diagnostics": "1.0.0-rc2-final",
    "Microsoft.EntityFrameworkCore": "1.0.0-rc2-final",
    "Microsoft.AspNetCore.StaticFiles": "1.0.0-rc2-final",
    "Microsoft.AspNetCore.Http": "1.0.0-rc2-final",
    "Microsoft.AspNetCore.Session": "1.0.0-rc2-final",
    "Microsoft.Extensions.Configuration.Json": "1.0.0-rc2-final",
    "Microsoft.Extensions.Logging": "1.0.0-rc2-final",
    "Microsoft.Extensions.Logging.Console": "1.0.0-rc2-final",
    "Microsoft.Extensions.WebEncoders": "1.0.0-rc2-final",
    "Newtonsoft.Json": "8.0.3",
    "Microsoft.AspNetCore.Razor": "1.0.0-rc2-final",
    "VitalChoice.Ecommerce.Domain": "1.0.0-*",
    "VitalChoice.Infrastructure.Domain": "1.0.0-*",
    "VitalChoice.Data": "1.0.0-*",
    "VitalChoice.Infrastructure": "1.0.0-*",
    "VitalChoice.Core": "1.0.0-*",
    "VitalChoice.Validation": "1.0.0-*",
    "VitalChoice.SharedWeb": "1.0.0-*"
  },
  "frameworks": {
    "net461": {
    },
    "netcoreapp1.0": {
      "imports": [
        "dnxcore50",
        "portable-net45+win8"
      ],
      "dependencies": {
        "Microsoft.NETCore.Platforms": {
          "type": "platform",
          "version": "1.0.1-rc2-*"
        },
        "System.Runtime": "4.1.0-rc2-*"
      }
    }
  },
  "commands": {
    "web": "Microsoft.AspNet.Server.Kestrel",
    "kestrel": "Microsoft.AspNet.Hosting --server Microsoft.AspNet.Server.Kestrel --server.urls http://localhost:5010",
    "gen": "Microsoft.Extensions.CodeGeneration",
    "ef": "EntityFramework.Commands"
  }
}
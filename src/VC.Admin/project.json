{
  "webroot": "wwwroot",
  "version": "1.0.0-*",
  "exclude": [
    "wwwroot",
    "node_modules",
    "bower_components",
    "deploy"
  ],
  "publishExclude": [
    "node_modules",
    "bower_components",
    "deploy",
    "**.kproj",
    "**.user",
    "**.vspscc"
  ],
  "dependencies": {
    "Microsoft.AspNet.Server.Kestrel": "1.0.0-*",
    "Microsoft.AspNet.IISPlatformHandler": "1.0.0-*",
    "Microsoft.AspNet.Mvc": "6.0.0-*",
    "Microsoft.AspNet.Diagnostics": "1.0.0-*",
    "Microsoft.AspNet.Diagnostics.Entity": "7.0.0-*",
    "Microsoft.AspNetCore.Identity.EntityFrameworkCore": "3.0.0-*",
    "Microsoft.AspNet.Server.WebListener": "1.0.0-*",
    "Microsoft.AspNet.StaticFiles": "1.0.0-*",
    "Microsoft.Extensions.Configuration.Json": "1.0.0-*",
    "Microsoft.Extensions.Logging.Console": "1.0.0-*",
    "EntityFramework.Core": "7.0.0-*",
    "VitalChoice.Ecommerce.Domain": "1.0.0-*",
    "VitalChoice.Infrastructure.Domain": "1.0.0-*",
    "VitalChoice.Data": "1.0.0-*",
    "VitalChoice.Infrastructure": "1.0.0-*",
    "VitalChoice.Core": "1.0.0-*",
    "VitalChoice.Validation": "1.0.0-*",
    "VitalChoice.DynamicData": "1.0.0-*",    
    "VitalChoice.SharedWeb": "1.0.0-*"
  },
  "frameworks": {
    "dnx451": {
      "dependencies": {
        "EntityFramework.Commands": "7.0.0-*"
      }
    },
    "netstandard1.5": {
      "dependencies": {
        "System.Runtime": "4.0.21-*",
        "System.Net.Http": "4.0.1-*"
      }
    }
  },
  "commands": {
    "web": "Microsoft.AspNet.Server.Kestrel",
    "kestrel": "Microsoft.AspNet.Hosting --server Microsoft.AspNet.Server.Kestrel --server.urls http://localhost:5504",
    "gen": "Microsoft.Extensions.CodeGeneration",
    "ef": "EntityFramework.Commands"
  }
}
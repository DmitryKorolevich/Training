{
  "webroot": "wwwroot",
  "version": "1.0.0-*",
  "buildOptions": {
    "compile": {
      "exclude": [
        "wwwroot",
        "node_modules",
        "bower_components",
        "deploy"
      ]
    },
    "copyToOutput": {
      "include": [
        "config.json",
        "web.config",
        "CSharpClassTemplate.tcs",
        "CSharpPreparseTemplate.tcs"
      ]
    },
    "preserveCompilationContext": true,
    "emitEntryPoint": true
  },
  "runtimeOptions": {
    "gcServer": false,
    "gcConcurrent": true
  },
  "packOptions": {
    "exclude": [
      "node_modules",
      "bower_components",
      "deploy",
      "**.xproj",
      "**.user",
      "**.vspscc"
    ]
  },
  "publishOptions": {
    "include": [
      "wwwroot",
      "config.json",
      "web.config",
      "CSharpClassTemplate.tcs",
      "CSharpPreparseTemplate.tcs"
    ]
  },
  "dependencies": {
    "Microsoft.AspNetCore.Server.Kestrel": "1.0.0-rc2-final",
    "Libuv": "1.9.0-rc2-20896",
    "Microsoft.AspNetCore.Server.IISIntegration": "1.0.0-rc2-final",
    "Microsoft.AspNetCore.Identity.EntityFrameworkCore": "1.0.0-fixed-*",
    "Microsoft.AspNetCore.Identity": "1.0.0-fixed-*",
    "Microsoft.AspNetCore.Mvc": "1.0.0-rc2-final",
    "Microsoft.AspNetCore.Diagnostics": "1.0.0-rc2-final",
    "Microsoft.AspNetCore.DataProtection": "1.0.0-rc2-final",
    "Microsoft.AspNetCore.StaticFiles": "1.0.0-rc2-final",
    "Microsoft.Extensions.Configuration.Json": "1.0.0-rc2-final",
    "Microsoft.Extensions.Logging.Console": "1.0.0-rc2-final",
    "Microsoft.EntityFrameworkCore": "1.0.0-fixed-*",
    "VitalChoice.Ecommerce.Domain": "1.0.0-*",
    "VitalChoice.Infrastructure.Domain": "1.0.0-*",
    "VitalChoice.Data": "1.0.0-*",
    "VitalChoice.Infrastructure": "1.0.0-*",
    "VitalChoice.Core": "1.0.0-*",
    "VitalChoice.Validation": "1.0.0-*",
    "VitalChoice.DynamicData": "1.0.0-*",
    "VitalChoice.SharedWeb": "1.0.0-*",
    "Microsoft.Extensions.Configuration.EnvironmentVariables": "1.0.0-rc2-final"
  },
  "frameworks": {
    "net451": {
    },
    "netcoreapp1.0": {
      "imports": [
        "dnxcore50",
        "portable-net45+win8"
      ],
      "dependencies": {
        "Microsoft.NETCore.Platforms": {
          "type": "platform",
          "version": "1.0.1-*"
        },
        "System.Runtime": "4.1.0-rc2-*",
        "System.Net.Http": "4.0.1-rc2-*"
      }
    }
  },
  "tools": {
    "Microsoft.AspNetCore.Server.IISIntegration.Tools": {
      "version": "1.0.0-*",
      "imports": "portable-net45+wp80+win8+wpa81+dnxcore50"
    }
  },
  "scripts": {
    "postpublish": "dotnet publish-iis --publish-folder %publish:OutputPath% --framework %publish:FullTargetFramework%"
  }
}
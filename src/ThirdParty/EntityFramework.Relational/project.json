{
    "version": "7.0.0-beta8",
    "description": "Shared Entity Framework components for relational data stores.",
    "repository": {
        "type": "git",
        "url": "git://github.com/aspnet/entityframework"
    },
    "compilationOptions": {
        "warningsAsErrors": true
    },
    "dependencies": {
        "EntityFramework.Core": "7.0.0-beta8"
    },
    "compile": "..\\Shared\\*.cs",
    "namedResource": {
        "EntityFramework.Relational.RelationalStrings": "Properties/RelationalStrings.resx"
    },
    "resource": [
        "Properties/EntityFramework.Relational.rd.xml"
    ],
    "frameworks": {
      "net45": {
        "frameworkAssemblies": {
          "System.Data": "4.0.0.0",
          "System.Transactions": "4.0.0.0"
        }
      },
      "dnx451": {
            "frameworkAssemblies": {
                "System.Data": "4.0.0.0",
                "System.Transactions": "4.0.0.0"
            }
        },
        "dnxcore50": {
            "dependencies": {
                "System.Data.Common": "4.0.1-beta-23409",
                "System.Text.RegularExpressions": "4.0.11-beta-23409",
                "Microsoft.CSharp": "4.0.1-beta-23409"
            }
        },
        "dotnet": {
            "dependencies": {
                "Microsoft.CSharp": "4.0.1-beta-23409",
                "System.Data.Common": "4.0.1-beta-23409",
                "System.Dynamic.Runtime": "4.0.11-beta-23409"
            }
        }
    }
}